-- Create the user 
create user BONDARENKO_DYU
  default tablespace USERS
  temporary tablespace TEMP
  profile DEFAULT
  quota 100m on bodt_dir_d
  quota 100m on users;
-- Grant/Revoke object privileges 
grant execute on BO_DATA.PK_ETL_API to BONDARENKO_DYU;
grant select, insert, update, delete, references, alter, index on BO_DATA.T_CDS_TABLE_DATE_LOG to BONDARENKO_DYU;
-- Grant/Revoke role privileges 
grant connect to BONDARENKO_DYU;
grant dba to BONDARENKO_DYU;
grant resource to BONDARENKO_DYU;
-- Grant/Revoke system privileges 
